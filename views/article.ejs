<%- include("header") %>
    <div class="content">
        <h1>Selected article</h1>
        <table id="category">
            <thead>
                <tr>
                    <th scope="col">Name</th>
                    <th scope="col">Category</th>
                    <th scope="col">Reference</th>
                    <th scope="col">Supplier</th>
                    <th scope="col">Price</th>
                    <th scope="col">VAT</th>
                    <th scope="col">Stock</th>
                    <th scope="col">Critical Stock</th>
                </tr>
            </thead>

            <tbody>
                <tr>
                    <td>
                        <%= rows.name %>
                    </td>
                    <td>
                        <%= rows.category %>
                    </td>
                    <td>
                        <%= rows.reference %>
                    </td>
                    <td>
                        <%= rows.supplier %>
                    </td>
                    <td>
                        <%= rows.price %>
                    </td>
                    <td>
                        <%= rows.vat %>
                    </td>
                    <td>
                        <%= rows.stock %>
                    </td>
                    <td>
                        <%= rows.critical_stock %>
                    </td>
                </tr>
            </tbody>

        </table>
    </div>
    <br>
    <%- include('footer') %>